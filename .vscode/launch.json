{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "vscode-jest-tests",
            "request": "launch",
            "runtimeExecutable": "yarn",
            // // "runtimeArgs": [
            //     "node",
            // // ],
            // "program": "${workspaceFolder}/.yarn/unplugged/jest-npm-24.9.0-8ddb425e99/node_modules/jest/bin/jest",
            "args": [
                
                "test",
                "--runInBand"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "disableOptimisticBPs": true
        },
       
        {
            "type": "node",
            "request": "launch",
            "name": "Example: Matching results",
            "runtimeExecutable": "yarn",
            "sourceMaps": true,
            "runtimeArgs": [
                "node",
                "-r",
                "ts-node/register",
            ],
            "skipFiles": [
                "<node_internals>/**",
            ],
            "env": {
                "TS_NODE_PROJECT": "./tsconfig.json",
            },
            "args": [
                "${workspaceFolder}/src/examples/matching-results.ts",
            ],
            "windows": {
                "args": ["${workspaceFolder}\\src\\examples\\matching-results.ts",]
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch match-domain-errors",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "node",
                "-r",
                "ts-node/register",
            ],
            "env": {
                "TS_NODE_PROJECT": "./tsconfig.json",
            },
            "args": [
                "${workspaceFolder}/src/examples/match-domain-errors.ts",
            ],
            "windows": {
                "args": ["${workspaceFolder}\\src\\examples\\match-domain-errors.ts",]
            }
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch typical pipeline",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "node",
                "-r",
                "ts-node/register",
            ],
            "env": {
                "TS_NODE_PROJECT": "./tsconfig.json",
            },
            "args": [
                "${workspaceFolder}/src/examples/typical-pipeline.ts",
            ],
            "windows": {
                "args": ["${workspaceFolder}\\src\\examples\\typical-pipeline.ts",]
            },
            "console": "integratedTerminal"
        }
    ]
}